<atlassian-plugin key="com.atlassian.confluence.plugins.confluence-quicknav" name="Confluence Quicknav" plugins-version="2"><plugin-info><param name="atlassian-data-center-compatible">true</param><description>This plugin provides and controls the QuickNav services to Confluence.</description><version>6.4.3</version><vendor name="Atlassian" url="https://www.atlassian.com/"/></plugin-info><component-import name="Content Name Search Service" key="contentNameSearchService" interface="com.atlassian.confluence.search.contentnames.ContentNameSearchService"/><rest key="quick-nav-rest" name="Confluence QuickNav API" path="/quicknav" version="1"><description>Provides QuickNav REST endpoints for Confluence</description></rest><contentname-search-sections-provider key="admin-section-provider" class="com.atlassian.confluence.plugins.quicknav.admin.AdminItemSearchSectionProvider"><description>Adds a section for search results for admin items into the quick nav</description></contentname-search-sections-provider><keyboard-shortcut key="quicksearch" i18n-name="admin.keyboard.shortcut.quicksearch.name" name="Quick Search"><order>40</order><description key="admin.keyboard.shortcut.quicksearch.desc">Sets focus on quicknav via gg keyboard shortcut</description><shortcut>gg</shortcut><operation type="moveToAndFocus">#quick-search-query</operation><context>global</context><hidden>true</hidden></keyboard-shortcut><web-resource key="quicknav-admin-resources" name="Resources for Quick nav admin section"><context>atl.general</context><context>atl.admin</context><resource name="quicknav-admin.css" type="download" location="css/quicknav-admin.css"/></web-resource><web-resource key="quicknav-resources" name="Resources for Quick nav"><context>atl.general</context><context>atl.admin</context><resource type="soy" name="quicknav-panel" location="soy/panel.soy"/></web-resource><web-panel key="quicknav-panel" name="QuickNav Panel" location="system.header/right/panels" weight="500"><resource name="view" type="soy" location="com.atlassian.confluence.plugins.confluence-quicknav:quicknav-resources/Confluence.Templates.QuickNav.panel"/><condition class="com.atlassian.confluence.plugin.descriptor.web.conditions.IsConfluenceSpacesLicensedCondition"/><condition class="com.atlassian.confluence.plugin.descriptor.web.conditions.user.RemoteUserCanUseConfluenceCondition"/></web-panel></atlassian-plugin>